<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <title>KeepDiet</title>
        <!-- Bootstrap CSS CDN -->
        <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
        <!-- Our Custom CSS -->
        <link rel="stylesheet" href="/partials/style.css">
        <!-- Scrollbar Custom CSS -->
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/malihu-custom-scrollbar-plugin/3.1.5/jquery.mCustomScrollbar.min.css">
    </head>
    <body>
        <div class="wrapper">
            <nav id="sidebar">
                <div class="sidebar-header">
                    <h3>KeepDiet</h3>
                </div>
                <ul class="list-unstyled components">
                    <p>Dummy Heading</p>
                    <li>
                        <a href="/">Home</a>
                    </li>
                    <li>
                        <a href="/groups">Groups</a>
                    </li>
                    <li class="active">
                        <a href="/reward" data-toggle="collapse" aria-expanded="false">Reward</a>
                        <ul class="collapse list-unstyled" id="rewardSubmenu">
                            <li><a href="/reward">Home</a></li>
                            <li><a href="/reportprogress">Report Progress</a></li>
                            <li><a href="/groupprogress">Group Progress</a></li>
                            <li><a href="/deposit">Deposits</a></li>
                        </ul>
                    </li>
                </ul>
            </nav>
            <div id="content">
                <nav class="navbar navbar-default">
                    <div class="container-fluid">
                        <div class="collapse navbar-collapse" id="loginBar">
                            <ul class="nav navbar-nav navbar-right">
                                <% if(!currentUser){ %>
                                <li><a href="/login">Login</a></li>
                                <li><a href="/register">Sign Up</a></li>
                                <% } else { %>
                                <li><a href="#">Signed As <%= currentUser.username %></a></li>
                                <li><a href="/logout">Logout</a></li>
                                <% } %>
                            </ul>
                        </div>
                    </div>
                </nav>
                <div style="margin-left:25%; padding:1px 16px; height: 1000px;">
                    <h2>Run meet at Sac Field</h2>
                    <h3>4/01/2018 ~ 4/30/2018</h3>
                    <h3>everyday except weekends at 7:00am</h3>
                    <h3>Goal: lose 20 lbs</h3>
                    <p>Members:</p>
                    <p>Tony Kim</p>
                    <p>Haoting Li</p>
                    <p>Junlin Liu</p>
                    <p>Wanyang Guo</p>
                    <p>Xiangyu Kong</p>
                </div>
            </div>
        </div>
        <!-- jQuery CDN -->
        <script src="https://code.jquery.com/jquery-1.12.0.min.js"></script>
        <!-- Bootstrap Js CDN -->
        <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
        <!-- jQuery Custom Scroller CDN -->
        <script src="https://cdnjs.cloudflare.com/ajax/libs/malihu-custom-scrollbar-plugin/3.1.5/jquery.mCustomScrollbar.concat.min.js"></script>

        <script type="text/javascript">
            $(document).ready(function () {
                $("#sidebar").mCustomScrollbar({
                    theme: "minimal"
                });

                $('#sidebarCollapse').on('click', function () {
                    $('#sidebar, #content').toggleClass('active');
                    $('.collapse.in').toggleClass('in');
                    $('a[aria-expanded=true]').attr('aria-expanded', 'false');
                });
            });
        </script>        
    </body>
</html>

