<%include ../partials/header %>

<script type="text/javascript">
    $(document).ready(function () {
        $("#sidebar").mCustomScrollbar({
            theme: "minimal"
        });

        $('#sidebarCollapse').on('click', function () {
            $('#sidebar, #content').toggleClass('active');
            $('.collapse.in').toggleClass('in');
            $('a[aria-expanded=true]').attr('aria-expanded', 'false');
        });
    });
</script>
<div style="margin: 25px auto; padding:1px 16px; height: 1000px;">
   <h1 style="text-align: center">Diary Entries</h1>
   <!--<h1 style="text-align: center">Xiangyu's Diary Entries</h1>-->
   <div margin: "25px auto;">
		<table border="3" cellspacing="10" align="center">
			<tr>
				<th class="text-center" width="100">Date</th>
				<th class="text-center" width="100">Check-in (lbs)</th>
				<th class="text-center" width="100">Check-out (lbs)</th>
				<th class="text-center" width="100">Breakfast</th>
				<th class="text-center" width="100">Lunch</th>
				<th class="text-center" width="100">Dinner</th>
				<th class="text-center" width="100">Snack</th>
				<th class="text-center" width="100">Exercise</th>
				<th class="text-center" width="100">Edit</th>
			</tr>
			<% diaries.forEach(function(diary){ %>
		    <tr class="text-center">
			        <td><%= diary.date %></td>
			        <td><%= diary.checkin %></td>
			        <td><%= diary.checkout %></td>
			        <td><%= diary.breakfast %></td>
			        <td><%= diary.lunch %></td>
			        <td><%= diary.dinner %></td>
			        <td><%= diary.snack %></td>
			        <td><%= diary.exercise %></td>
			        <td><a class="btn btn-primary btn-small" href="/reward/reportprogress/edit/<%= diary._id %>">Edit</a></td>
		    </tr>
			<% }); %>
		</table>
		<a class="btn btn-primary btn-large" href="/reward/reportprogress/new">Add New Entry</a>
	</div>
</div>
<% include ../partials/footer %>